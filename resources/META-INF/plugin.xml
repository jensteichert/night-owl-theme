  <extensions defaultExtensionNs="com.intellij">
    <!-- Add your extensions here -->
    <themeProvider id="a007a5d1-9d21-418a-bccf-f7d95be29907" path="/Night_Owl.theme.json"/>
  </extensions>

  <actions>
    <!-- Add your actions here -->
  </actions>

